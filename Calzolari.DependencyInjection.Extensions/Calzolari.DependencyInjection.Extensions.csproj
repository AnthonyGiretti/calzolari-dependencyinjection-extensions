<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>5.0.1</Version>
    <Authors>Anthony Giretti</Authors>
    <Company>Calzolari</Company>
    <Product>Calzolari.DependencyInjection.Extensions</Product>
    <Description>Extensions to manage registered instance</Description>
    <PackageProjectUrl>https://github.com/AnthonyGiretti/calzolari-dependencyinjection-extensions</PackageProjectUrl>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <PackageTags>dotnet;IServiceCollection</PackageTags>
    <RepositoryUrl>https://github.com/AnthonyGiretti/calzolari-dependencyinjection-extensions</RepositoryUrl>
    <PackageIcon>calzolari.png</PackageIcon>
    <PackageIconUrl />
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="5.0.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\calzolari.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
